	BLOQUEO_EDITAR int DEFAULT 0 NULL,
	ESTADO int DEFAULT 0 NULL,
	CARNAZA_SALADA decimal(10,4) NULL,
	DESCARNE_ADHERIDO decimal(10,4) NULL,
